FUNCTION InsertionSort(array)
	FOR j = 1 IN [0 ..< array.length]
		key = array[j]
		i = j - 1
		WHILE i >= 0 && array[i] > key
			array[i + 1] = array[i]
			i = i - 1
			array[i + 1] = key
	RETURN array
